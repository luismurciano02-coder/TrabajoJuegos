{% extends 'base.html.twig' %}

{% block title %}Aplicaciones{% endblock %}

{% block body %}
    <h1>Aplicaciones</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ aplicacione.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ aplicacione.nombre }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ aplicacione.activo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Apikey</th>
                <td>{{ aplicacione.apikey }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_aplicaciones_index') }}">back to list</a>

    <a href="{{ path('app_aplicaciones_edit', {'id': aplicacione.id}) }}">edit</a>

    {{ include('aplicaciones/_delete_form.html.twig') }}
{% endblock %}
